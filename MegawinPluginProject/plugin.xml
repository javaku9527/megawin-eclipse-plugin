<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="Megawin.commands.flashMemoryCommand"
            name="Flash Memory">
      </command>
      <command
            id="Megawin.commands.command1"
            name="command1">
      </command>
   </extension>

   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.megawin.plugin.handlers.FlashMemoryHandler"
            commandId="Megawin.commands.flashMemoryCommand">
      </handler>
      <handler
            class="com.megawin.plugin.handlers.FlashMemoryHandler2"
            commandId="Megawin.commands.command1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="Megawin.commands.flashMemoryCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+7">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="Megawin.menus.sampleMenu"
               label="Megawin"
               mnemonic="M">
            <command
                  commandId="Megawin.commands.command1"
                  id="Megawin.menus.sampleCommand"
                  label="SampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="MegawinPluginProject.toolbars.flashMemory">
            <command
                  commandId="Megawin.commands.flashMemoryCommand"
                  disabledIcon="src/com/megawin/plugin/handlers/SampleHandler.java"
                  id="Megawin.toolbars.flashMemoryCommand"
                  label="Flash"
                  tooltip="Flash">
            </command>
            <command
                  commandId="Megawin.commands.command1"
                  id="Megawin.toolbars.flashMemoryCommand2"
                  label="Flash2"
                  tooltip="Flash">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.megawin.plugin.app.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="com.megawin.plugin.app.YourLaunchDelegate"
            id="MegawinPluginProject.launchConfigurationType1"
            name="788"
         	modes="run, debug">
      </launchConfigurationType>
   </extension>

</plugin>
