            name="name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <menu
               id="fileMenu"
               label="file">
            <command
                  commandId="com.megawin.plugin.flashmemory"
                  label="Flash"
                  style="push"
                  tooltip="Flash the memory">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.megawin.plugin.handlers.FlashHandler"
            commandId="com.megawin.plugin.handler1">
      </handler>
   </extension>
